%h1 Registered users
%table
  %tr
    %th Username
    %th Email
    %th First Name
    %th Last Name
    %th
  - @user.each do |user|
    %tr
      %td= user.username
      %td= user.email
      %td= user.firstname
      %td= user.lastname
      -  if current_user == user
        %td= link_to 'Show', user_path(current_user)

= link_to "Products", products_path